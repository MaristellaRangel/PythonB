temperaturas = []

for i in range(14):
    temperaturas.append(int(input(f"Digite a {i + 1}Âª temperatura:")))

menorTemp = temperaturas[0]
maiorTemp = temperaturas[0]

for temp in temperaturas:
    if temp < menorTemp:
        menorTemp = temp
    if temp > maiorTemp:
        maiorTemp = temp
print(f"Maior temperatura: {maiorTemp}")
print(f"Menor temperatura: {menorTemp}")
